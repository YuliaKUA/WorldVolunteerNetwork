name: dockercompose16128883506837845994
services:
  minio:
    command:
      - server
      - --console-address
      - :9001
      - /data
    container_name: minio
    image: minio/minio
    networks:
      default: null
    ports:
      - mode: ingress
        target: 9000
        published: "9000"
        protocol: tcp
      - mode: ingress
        target: 9001
        published: "9001"
        protocol: tcp
    restart: always
    volumes:
      - type: volume
        source: minio_data
        target: /data
        volume: {}
  postgres:
    container_name: postgres
    environment:
      POSTGRES_DB: world_volunteer_network
      POSTGRES_PASSWORD: postgres
      POSTGRES_USER: postgres
    image: postgres
    networks:
      default: null
    ports:
      - mode: ingress
        target: 5432
        published: "5432"
        protocol: tcp
    restart: always
    volumes:
      - type: bind
        source: C:\Project\net\WorldVolunteerNetwork\postgres_data
        target: /var/lib/postgresql/data
        bind:
          create_host_path: true
networks:
  default:
    name: dockercompose16128883506837845994_default
volumes:
  minio_data:
    name: dockercompose16128883506837845994_minio_data